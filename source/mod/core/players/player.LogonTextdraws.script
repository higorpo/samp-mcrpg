        PlayerTextDrawShow(playerid, logonIndex_basePlayer[playerid][16]);
        PlayerTextDrawShow(playerid, logonIndex_basePlayer[playerid][17]);




        // Função que irá chamar as informações de cada tela.
        HideLogon(playerid, tela_community);
        HideLogon(playerid, tela_inicio);
        HideLogon(playerid, tela_register);
        ShowLogon(playerid, tela_credits);
        return 1;
    }



    // Textbox login
    if(playertextid == logonLogin_Player[playerid][0])
    {
        ShowPlayerDialog(playerid, @lDialog_PlayerName, DIALOG_STYLE_INPUT, "{FF0000}#{FFFFFF} Insira seu nick", 
            "{FFFFFF}Coloque o nome do seu personagem no jogo, exemplo: \"Higor_Oliveira\".", "Pronto", "Sair");
        return 1;
    }
    if(playertextid == logonLogin_Player[playerid][1])
    {
        ShowPlayerDialog(playerid, @lDialog_Password, DIALOG_STYLE_PASSWORD, "{FF0000}#{FFFFFF} Coloque sua senha", 
            "{FFFFFF}Coloque a senha que você usou para se registrar no Majestic:", "Pronto", "Sair");
        return 1;
    }



    // Textbox registro
    if(playertextid == logonRegister_Player[playerid][0])
    {
        // Nome sobrenome
        ShowPlayerDialog(playerid, @rDialog_PlayerName, DIALOG_STYLE_INPUT, "{FF0000}#{FFFFFF} Insira um novo nick", 
            "{FFFFFF}Coloque o nome desejado para que seja usado como nome pelo seu personagem.\n\
            Exemplo: \"Higor_Oliveira\":", "Pronto", "Sair");
        return 1;
    }
    if(playertextid == logonRegister_Player[playerid][1])
    {
        // senha
        ShowPlayerDialog(playerid, @rDialog_Password, DIALOG_STYLE_PASSWORD, "{FF0000}#{FFFFFF} Coloque uma senha", 
            "{FFFFFF}Coloque uma senha segura & confiavel, de preferencia que somente você saiba-a.\n\n\
            {23B528}* Lembre-se que as senhas devem ter entre 5 a 20 caracteres\n\
            * Se você esquecer sua senha, você poderá recupera-la usando o Minha Yeeah\n\
            {F35353}* Não use a mesma senha que você utiliza em outros sites, principalmente no Minha Yeeah\n\
            * Não divulgue sua senha para ninguém, nem mesmo para seus amigos\n", "Pronto", "Sair");
        return 1;
    }
    if(playertextid == logonRegister_Player[playerid][2])
    {
        // E-mail
        ShowPlayerDialog(playerid, @rDialog_Email, DIALOG_STYLE_INPUT, "{FF0000}#{FFFFFF} Coloque um e-mail valido", 
            "{FFFFFF}Por-favor, coloque um e-mail valido:", "Pronto", "Sair");
        return 1;
    }




    // Botão voltar da textdraw da segunda tela de registro. Ela não precisava ser clicavel, mais eu decidi por pra depois poder destruí-las e ter espaço para textdraws.
    if(playertextid == logonRegister_two@oldTxt[26])
    {
        if(GetPlayerTelaID(playerid) == tela_register@confirmmail || GetPlayerTelaID(playerid) == tela_register@inputdados)
        {
            HideLogon(playerid, tela_register@lastregisters);
            DestroyPlayerTextdrawsLogon(playerid, tela_register@lastregisters);
            if(GetPlayerTelaID(playerid) == tela_register@confirmmail)
            {
                HideLogon(playerid, tela_register@confirmmail);
                DestroyPlayerTextdrawsLogon(playerid, tela_register@confirmmail);
            }
            else 
            {
                HideLogon(playerid, tela_register@inputdados);
                DestroyPlayerTextdrawsLogon(playerid, tela_register@inputdados);
            }

            CreatePlayerTextdrawsLogon(playerid, tela_register);
            ShowLogon(playerid, tela_register);


            /* Salva os valores */
            new playerMail[41];
            GetPVarString(playerid, "register@email", playerMail, sizeof(playerMail));
            new playerName[MAX_PLAYER_NAME];
            GetPVarString(playerid, "register@playername", playerName, sizeof(playerName));
            new playerPassword[20];
            GetPVarString(playerid, "register@password", playerPassword, sizeof(playerPassword));

            PlayerTextDrawSetString(playerid, logonRegister_Player[playerid][0], strlen(playerName) > 0 ? playerName : "Nome_Sobrenome");
            PasswordEncode(playerPassword, strlen(playerPassword));
            PlayerTextDrawSetString(playerid, logonRegister_Player[playerid][1], strlen(playerPassword) > 0 ? playerPassword : "]]]]]]]");
            new string[41];
            strmid(string, playerMail, 0, strfind(playerMail, "@"));
            format(playerMail, sizeof(playerMail), string);
            PlayerTextDrawSetString(playerid, logonRegister_Player[playerid][2], strlen(playerMail) > 0 ? playerMail : "e-mail");
        }
        return 1;
    }
    if(playertextid == logonRegister_two@cnfMail[9])
    {
        if(GetPlayerTelaID(playerid) == tela_register@confirmmail)
        {
            HideLogon(playerid, tela_register@lastregisters);
            HideLogon(playerid, tela_register@confirmmail);
            DestroyPlayerTextdrawsLogon(playerid, tela_register@lastregisters);
            DestroyPlayerTextdrawsLogon(playerid, tela_register@confirmmail);

            CreatePlayerTextdrawsLogon(playerid, tela_base);
            CreatePlayerTextdrawsLogon(playerid, tela_inicio);
            ShowLogon(playerid, tela_base);
            ShowLogon(playerid, tela_inicio);

            new 
                name@saved[MAX_PLAYER_NAME],
                pass@saved[21]
            ;

            GetPVarString(playerid, "login@playername", name@saved, MAX_PLAYER_NAME);
            GetPVarString(playerid, "login@password", pass@saved, 21);

            PlayerTextDrawSetString(playerid, logonLogin_Player[playerid][0], strlen(name@saved) > 0 ? name@saved : "Nome_Sobrenome");
            PasswordEncode(pass@saved, strlen(pass@saved));   
            PlayerTextDrawSetString(playerid, logonLogin_Player[playerid][1], strlen(pass@saved) > 0 ? pass@saved : "]]]]]]]"); 
        }
        return 1;
    }


    // Botão de ajuda confirmação do e-mail 
    if(playertextid == logonRegister_two@cnfMail[16])
    {
        new string[308];
        strcat(string, "{FFFFFF}Para confirmar seu e-mail, vá até o endereço de e-mail digitado, procure na sua caixa de entrada\n");
        strcat(string, "ou na lixeira, um e-mail de ativação do e-mail. Após isso, clique sobre o link informado com o token e pronto!\n");
        strcat(string, "Depois, é só voltar para o servidor e já estará tudo confirmado. Simples e rápido, não é?!");

        ShowPlayerDialog(playerid, INVALID_DIALOG, DIALOG_STYLE_MSGBOX, "{FF0000}# {FFFFFF}Alerta", string, "Fechar", "");
        return 1;
    }
    return 1;
}

