DestroyPlayerTextdrawsLogon(playerid, telaid)
{
    switch(telaid)
    {
        case tela_base:
        {
            for(new i = 0; i < 18; ++i) PlayerTextDrawDestroy(playerid, logonIndex_basePlayer[playerid][i]);
            return 1;
        }
        case tela_inicio:
        {
            for(new i = 0; i < 2; ++i) PlayerTextDrawDestroy(playerid, logonLogin_Player[playerid][i]);
            DestroyPlayerTextdrawsNewsIndex(playerid);
            return 1;
        }
        case tela_community:
        {
            //null
            return 1;
        }
        case tela_credits:
        {
            //null
            return 1;
        }
        case tela_register:
        {
            for(new i = 0; i < 3; ++i) PlayerTextDrawDestroy(playerid, logonRegister_Player[playerid][i]);
            return 1;
        }
        case tela_register@lastregisters:
        {
            for(new i = 0; i < 27; ++i) PlayerTextDrawDestroy(playerid, logonRegister_two@oldTxt[i]);
            for(new i = 0; i < 11; ++i) PlayerTextDrawDestroy(playerid, logonRegister_two@flexa[i]);
            return 1;
        }
        case tela_register@confirmmail:
        {
            for(new i = 1; i < 17; i++) PlayerTextDrawDestroy(playerid, logonRegister_two@cnfMail[i]);
            return 1;
        }
        case tela_register@inputdados:
        {
            return 1;
        }
    }
    return 1;
}

ShowLogon(playerid, telaID)
{
    switch(telaID)
    {
        case tela_base:
        {
            SelectTextDraw(playerid, 0xFFFFFFFF);
            for(new i = 0; i < 9; i++) TextDrawShowForPlayer(playerid, logonIndex_base[i]);
            for(new i = 0; i < 18; i++) PlayerTextDrawShow(playerid, logonIndex_basePlayer[playerid][i]);
            return 1;
        }
        case tela_inicio:
        {
            SetPVarInt(playerid, "telaLogon_telaid", tela_inicio);

            for(new i = 0; i < 18; i++) TextDrawShowForPlayer(playerid, logonLogin[i]);
            for(new i = 0; i < 2; i++) PlayerTextDrawShow(playerid, logonLogin_Player[playerid][i]);

            ShowNewsInIndex(playerid); // Carrega a base das noticias.
            ShowTextdrawsInfoServer(playerid); // Carrega as informações dos últimos cadastrados, e quantidade de casas/contas/empresas do servidor.
            return 1;
        }
        case tela_register: // Primeira tela de registro, aquela que quando você clica na tela de 'login' no "registrar-se"
        {
            SetPVarInt(playerid, "telaLogon_telaid", tela_register);
            for(new i = 1; i < 35; i++) TextDrawShowForPlayer(playerid, logonRegister[i]);
            for(new i = 0; i < 3; i++) PlayerTextDrawShow(playerid, logonRegister_Player[playerid][i]);
        }
        case tela_register@lastregisters: // Tela a onde mostra as últimas informações que o player digitou na tela anterior. Aquela que tem a flexa.
        {
            for(new i = 0; i < 27; i++) PlayerTextDrawShow(playerid, logonRegister_two@oldTxt[i]);
            for(new i = 0; i < 11; i++) PlayerTextDrawShow(playerid, logonRegister_two@flexa[i]);
            return 1;
        }
        case tela_register@confirmmail: // Tela que faz a confirmação do e-mail.
        {
            SetPVarInt(playerid, "telaLogon_telaid", tela_register@confirmmail);
            for(new i = 1; i < 17; i++) PlayerTextDrawShow(playerid, logonRegister_two@cnfMail[i]);


            /* Salva os valores */
            new playerName[41];
            GetPVarString(playerid, "register@playername", playerName, sizeof(playerName));

            new playerMail[41];
            GetPVarString(playerid, "register@email", playerMail, sizeof(playerMail));

            new playerPassword[20];
            GetPVarString(playerid, "register@password", playerPassword, sizeof(playerPassword));


            /* Altera os valores */
            PlayerTextDrawSetString(playerid, logonRegister_two@oldTxt[15], playerName);
            PlayerTextDrawSetString(playerid, logonRegister_two@oldTxt[20], playerPassword);
            PlayerTextDrawSetString(playerid, logonRegister_two@oldTxt[25], playerMail);
            return 1;
        }
        case tela_register@inputdados:
        {
            return 1;
        }
    }
    return 1;
}

