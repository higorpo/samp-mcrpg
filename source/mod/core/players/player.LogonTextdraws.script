SetPlayerInfos(playerid, bool:register = false, info_id, text[]) // Função que irá setar o que for pedido nas textdraws, exemplo: Nickname.
{
    switch(info_id)
    {
        case textdraw_Playername:
        {
            if(register == true)
            {
                // Dados do registro.
                SetPlayerName(playerid, text);
                PlayerTextDrawSetString(playerid, logonRegister_Player[playerid][0], text);
                return 1;
            }
            else 
            {
                // Dados do login.
                SetPlayerName(playerid, text);
                PlayerTextDrawSetString(playerid, logonLogin_Player[playerid][0], text);
            }
        }
        case textdraw_Password:
        {
            if(register == true)
            {
                // Dados do registro.
                PasswordEncode(text, strlen(text));
                PlayerTextDrawSetString(playerid, logonRegister_Player[playerid][1], text);
                return 1;
            }
            else 
            {
                // Dados do login.
                PasswordEncode(text, strlen(text));
                PlayerTextDrawSetString(playerid, logonLogin_Player[playerid][1], text);
            }
        }

        //
        case textdraw_Email:
        {
            PlayerTextDrawSetString(playerid, logonRegister_Player[playerid][2], text);
        }
    }
    return 1;
}

