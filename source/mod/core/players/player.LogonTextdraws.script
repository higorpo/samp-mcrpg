hook OnGameModeInit()
{
    printc(CONSOLE_GREY, "-> Carregando textdraws da tela inicial.");

    CreateTextdrawsLogon(tela_base);
    CreateTextdrawsLogon(tela_inicio);
    CreateTextdrawsLogon(tela_register);
    return 1;
}

hook OnPlayerConnect(playerid)
{
    CreatePlayerTextdrawsLogon(playerid, tela_base);
    CreatePlayerTextdrawsLogon(playerid, tela_inicio);

    SetTimerEx("asdiuads", 1000, true, "i", playerid);
    return 1;
}

forward asdiuads(playerid);
public asdiuads(playerid)
{
    new string[121];
    format(string, sizeof(string), "Tela: %d", GetPlayerTelaID(playerid));
    SendClientMessage(playerid, -1, string);
    return 1;
}


hook OnPlayerClickTextDraw(playerid, Text:clickedid)
{
    if(_:clickedid == INVALID_TEXT_DRAW)
    {
        if(GetPlayerTelaID(playerid) != -1) return SelectTextDraw(playerid, 0xFFFFFFFF);
        return 1;
    }
    if(clickedid == logonIndex_base[8])
    {
        HideLogon(playerid, tela_base);
        HideLogon(playerid, tela_inicio);
        Kick(playerid);
        return 1;
    }


    /* Tela de login */
    if(clickedid == logonLogin[4]) // Botão de dúvida / nick.
    {
        // Colocar depois em textdraw.
        ShowPlayerDialog(playerid, INVALID_DIALOG, DIALOG_STYLE_MSGBOX, "Alerta", 
            "{FFFFFF}Coloque aqui o nome do seu personagem no campo.", "Fechar", "");
        return 1;
    }

    if(clickedid == logonLogin[15]) // Botão para entrar no servidor.
    {

        // Botão de entrar, login.
        new playername[MAX_PLAYER_NAME], 
            playerpassword[20]
        ;

        GetPVarString(playerid, "login@playername", playername, sizeof(playername));
        GetPVarString(playerid, "login@password", playerpassword, sizeof(playerpassword));


        /* Verifica se o usuário preencheu todos os campos corretamente. */
        if(strlen(playername) < 1 || strlen(playerpassword) < 1) return 0; // Exibe mensagem de erro;


        SetPlayerName(playerid, playername);


        /* Caso o player tenha preenchido todos os campos.*/
        new query[404+24];


        mysql_format(mysqlConnect, query, sizeof(query),"SELECT `id` FROM `accounts` WHERE `name` = '%e' AND BINARY `password` = sha1('%e')", 
            playername, 
            playerpassword);
        mysql_query(mysqlConnect, query); // query(consulta).


        if(cache_num_rows() == 0) // Se a senha estiver errada.
        {
            pInfo[playerid][errorPass] ++;
            SendClientMessage(playerid, -1, "[debug] Você errou sua senha.");
            // Caso o player erre a senha, envia uma mensagem para ele avisando-o que ele errou sua senha.

            if(pInfo[playerid][errorPass] == 4)
            {
                /* Esconde todas as textdraws de login. */
                HideLogon(playerid, tela_base);
                HideLogon(playerid, tela_inicio);
                HideLogon(playerid, tela_community);
                HideLogon(playerid, tela_register);
                HideLogon(playerid, tela_register@lastregisters);
                HideLogon(playerid, tela_register@confirmmail);
                HideLogon(playerid, tela_register@inputdados);
                HideLogon(playerid, tela_credits);

                DestroyPlayerTextdrawsLogon(playerid, tela_base);
                DestroyPlayerTextdrawsLogon(playerid, tela_inicio);
                DestroyPlayerTextdrawsLogon(playerid, tela_community);
                DestroyPlayerTextdrawsLogon(playerid, tela_register);
                DestroyPlayerTextdrawsLogon(playerid, tela_register@lastregisters);
                DestroyPlayerTextdrawsLogon(playerid, tela_register@confirmmail);
                DestroyPlayerTextdrawsLogon(playerid, tela_register@inputdados);
                DestroyPlayerTextdrawsLogon(playerid, tela_credits);

                ResetAllDadosLogon(playerid);
                Kick(playerid);
                return 1;
            }
        }

        else if(cache_num_rows() > 0) // Se a senha estiver correta.
        {   
            /* Pega as vars necessárias */
            pInfo[playerid][id] = cache_get_field_content_int(0, "id");


            /* Faz a query para carregar a conta do player.. */
            mysql_format(mysqlConnect, query, sizeof(query), "SELECT * FROM `accounts` \
                                                                INNER JOIN `accounts_data` ON `accounts`.`id` = `accounts_data`.`id_player` \
                                                                LEFT JOIN `accounts_rank` ON `accounts`.`id` = `accounts_rank`.`id_player` \
                                                                WHERE `accounts`.`name` = '%e'", playername); 
            mysql_pquery(mysqlConnect, query, "LoadPlayerAccount", "i", playerid);

            /* Outras configurações */
            ResetAllDadosLogon(playerid); // Reseta os dados do player.
        }
        return 1;
    }

    if(clickedid == logonLogin[17]) // Botão registrar-se.
    {
        if(GetPlayerTelaID(playerid) == tela_register) return 0;

        HideLogon(playerid, tela_base);
        HideLogon(playerid, tela_inicio);
        DestroyPlayerTextdrawsLogon(playerid, tela_base);
        DestroyPlayerTextdrawsLogon(playerid, tela_inicio);

        CreatePlayerTextdrawsLogon(playerid, tela_register);
        ShowLogon(playerid, tela_register); // mostra as textdraws de registro

        new 
            name@saved[MAX_PLAYER_NAME],
            pass@saved[21],
            mail@saved[41]
        ;

        GetPVarString(playerid, "register@playername", name@saved, MAX_PLAYER_NAME);
        GetPVarString(playerid, "register@password", pass@saved, 21);
        GetPVarString(playerid, "register@email", mail@saved, 41);


        PlayerTextDrawSetString(playerid, logonRegister_Player[playerid][0], strlen(name@saved) > 0 ? name@saved : "Nome_Sobrenome");
        PasswordEncode(pass@saved, strlen(pass@saved));   
        PlayerTextDrawSetString(playerid, logonRegister_Player[playerid][1], strlen(pass@saved) > 0 ? pass@saved : "]]]]]]]"); 
        new string[41];
        strmid(string, mail@saved, 0, strfind(mail@saved, "@"));
        format(mail@saved, sizeof(mail@saved), string);
        PlayerTextDrawSetString(playerid, logonRegister_Player[playerid][2], strlen(mail@saved) > 0 ? mail@saved : "e-mail");
        return 1;
    }


    /* Tela de registro */
    if(clickedid == logonRegister[9]) // Botão sair de registro; 
    {
        if(GetPlayerTelaID(playerid) == tela_register)
        {
            // Se ele estiveres na tela de registro

            HideLogon(playerid, tela_register);
            DestroyPlayerTextdrawsLogon(playerid, tela_register);

            CreatePlayerTextdrawsLogon(playerid, tela_base);
            CreatePlayerTextdrawsLogon(playerid, tela_inicio);

            ShowLogon(playerid, tela_base);
            ShowLogon(playerid, tela_inicio);

            new 
                name@saved[MAX_PLAYER_NAME],
                pass@saved[21]
            ;

            GetPVarString(playerid, "login@playername", name@saved, MAX_PLAYER_NAME);
            GetPVarString(playerid, "login@password", pass@saved, 21);

            PlayerTextDrawSetString(playerid, logonLogin_Player[playerid][0], strlen(name@saved) > 0 ? name@saved : "Nome_Sobrenome");
            PasswordEncode(pass@saved, strlen(pass@saved));   
            PlayerTextDrawSetString(playerid, logonLogin_Player[playerid][1], strlen(pass@saved) > 0 ? pass@saved : "]]]]]]]"); 


        }
        return 1;
    }

    if(clickedid == logonRegister[34]) // Botão registro
    {
        if(GetPlayerTelaID(playerid) == tela_register)
        {
            /* Salva os valores */
            new playerMail[41];
            GetPVarString(playerid, "register@email", playerMail, sizeof(playerMail));

            new playerName[MAX_PLAYER_NAME];
            GetPVarString(playerid, "register@playername", playerName, sizeof(playerName));

            new playerPassword[20];
            GetPVarString(playerid, "register@password", playerPassword, sizeof(playerPassword));

            /* Verifica se o usuário preencheu todos os campos corretamente. */
            if(strlen(playerName) < 1 || strlen(playerPassword) < 1 || strlen(playerMail) < 1) return SendClientMessage(playerid, -1, "Não preencheu tudo."); // Mensagem de erro



            /* Configurações para mandar pra proxima tela.  */


            // Tela que mostra os dados digitados na tela anterior.
            new confirmouMail = GetPVarInt(playerid, "confirmou__email@player"); // Verificar se o usuário já confirmou seu e-mail.

            HideLogon(playerid, tela_register);
            DestroyPlayerTextdrawsLogon(playerid, tela_register);
            
            CreatePlayerTextdrawsLogon(playerid, tela_register@lastregisters);
            ShowLogon(playerid, tela_register@lastregisters);

            PlayerTextDrawSetString(playerid, logonRegister_two@oldTxt[15], playerName);
            PlayerTextDrawSetString(playerid, logonRegister_two@oldTxt[20], playerPassword);
            PlayerTextDrawSetString(playerid, logonRegister_two@oldTxt[25], playerMail);


            if(confirmouMail == 0)
            {
                /* Confirmação do e-mail */

                // Cria o código.
                new codigo@cnf[21];
                format(codigo@cnf, 21, GetRandomString(20));
                SetPVarString(playerid, "codigo_cfn__mail", codigo@cnf);

                // Insere o código na tabela para aguardar a confirmação.
                new query[57+21];
                mysql_format(mysqlConnect, query, sizeof(query), "INSERT INTO `reg_confirm`(`token_confirmed`) VALUES ('%e');", codigo@cnf);
                mysql_query(mysqlConnect, query, false);

                // Mandar e-mail com o código token.


                // Timer para verificar se ele confirmou o e-mail.
                timer@codigo_cfn__mail[playerid] = SetTimerEx("VerifyIfCnfMail", 2000, true, "i", playerid);


                /* Mostra a tela para confirmação do e-mail. */
                CreatePlayerTextdrawsLogon(playerid, tela_register@confirmmail);
                ShowLogon(playerid, tela_register@confirmmail);
            }
            else
            {
                // Se ele confirmou o e-mail, mostra a tela para o player colocar as novas infos.
            }
            return 1;
        }
        return 1;
    }

    if(clickedid == logonRegister[18]) // Icone de ajuda usuário
    {

        new string[1150];
        strcat(string, "{FFFFFF}Coloque o nome que o seu personagem terá em sua nova vida no servidor.\nO nome do seu personagem deve ser um nome real, ");
        strcat(string, "de preferencia que seja seu nome real, porém se preferir colocar um nome ficticio,\nsaíba que ele deve conter as mesmas ");
        strcat(string, "caracteristicas de um nome real, ou seja, ele deverá conter Nome & Sobrenome.\nVocê ficará com este nick(nome) ");
        strcat(string, "até decidir troca-lo por um outro nick, seguindo as informações abaixo:\n{23B528}Modo legal: A troca pode ser feita em um cartório no servidor, porém de acordo ");
        strcat(string, "com as regras implementadas no servidor.\nA troca também custará a quantia de R$10.000 que será destinada aos ");
        strcat(string, "cofres públicos.\nFique ciente que a troca só poderá ser efetuada uma única vez, e que depois terá de ficar com este nome para sempre.\n");
        strcat(string, "{F35353}Modo ilegal: Caso não queira pagar tanto dinheiro, e precise trocar mais de uma vez o seu nick, você poderá contratar\num profissional na área de ");
        strcat(string, "disfarces que fornecerá a você um novo nome, e uma nova vida. Perfeito para fugitivos da polícia..\nDepois de trocar seu nome, todos os seus rastros do passado ficarão ");
        strcat(string, "totalmente apagados, facilitando assim sua fuga da polícia.");

        ShowPlayerDialog(playerid, INVALID_DIALOG, DIALOG_STYLE_MSGBOX, "{FF0000}# {FFFFFF}Alerta", string, "Fechar", "");
        return 1;
    }

    if(clickedid == logonRegister[27]) // icone de ajuda e-mail
    {
        new string[108+70+1];
        strcat(string, "{FFFFFF}Insira um endereço de e-mail valido, pois com este e-mail você poderá receber códigos promocionais\n");
        strcat(string, "{FFFFFF}recuperar sua conta, ver notícias em primeira mão e muito mais.");

        ShowPlayerDialog(playerid, INVALID_DIALOG, DIALOG_STYLE_MSGBOX, "{FF0000}# {FFFFFF}Alerta", string, "Fechar", "");
        return 1;
    }
    return 1;
}

